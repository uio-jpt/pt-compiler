aspect PrettyPrint {

       public void PTCompilationUnit.toString(StringBuffer sb) {
           super.toString(sb);
           for (PTDecl ptDecl: getPTDecls()) 
               ptDecl.toString(sb);
       }

       public void PTDecl.toString(StringBuffer sb) {
           sb.append("Template " + getName() + " { }\n");
       }
}
