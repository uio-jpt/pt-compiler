template T {
    required type Foo() {
        String f();
    } 

    class X() {
        public g(Foo f) {
            System.out.println(f.f());
        }
    }
}

package P {
    inst T with  Foo <= Bar;

    class Bar {
        String f() { return "hello world"; }
    }

    class Main {
        public static void main(String args[]) {
            new X().g( new Bar() );
        }
    }
}
